<p id="notice"><%= notice %></p>

<h1>Qr Code Positions</h1>

<table>
  <thead>
    <tr>
      <th>Page</th>
      <th>X</th>
      <th>Y</th>
      <th>Size</th>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @qr_code_positions.each do |qr_code_position| %>
      <tr>
        <td><%= qr_code_position.page %></td>
        <td><%= qr_code_position.x %></td>
        <td><%= qr_code_position.y %></td>
        <td><%= qr_code_position.size %></td>
        <td><%= qr_code_position.name %></td>
        <td><%= link_to 'Show', qr_code_position %></td>
        <td><%= link_to 'Edit', edit_qr_code_position_path(qr_code_position) %></td>
        <td><%= link_to 'Destroy', qr_code_position, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Qr Code Position', new_qr_code_position_path %>
